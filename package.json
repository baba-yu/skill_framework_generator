{
  "name": "skill_framework_generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:dictionary && vue-tsc -b && vite build",
    "build:dictionary": "node scripts/build-dictionary.js",
    "build:dictionary:watch": "node scripts/build-dictionary.js --watch",
    "update:keywords": "npm run build:dictionary",
    "preview": "vite preview",
    "dictionary:stats": "node -e \"console.log(JSON.stringify(JSON.parse(require('fs').readFileSync('public/data/dictionary-stats.json')), null, 2))\"",
    "dictionary:validate": "node scripts/validate-dictionary.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.90.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vue-tsc": "^3.0.5"
  }
}
